{
  "name": "@memberjunction/query-gen",
  "type": "module",
  "version": "5.2.0",
  "description": "AI-powered generation of domain-specific SQL query templates with automatic testing, refinement, and metadata export",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "mj-querygen": "./dist/cli/index.js"
  },
  "scripts": {
    "build": "tsc && tsc-alias -f",
    "watch": "tsc --watch",
    "clean": "rimraf dist",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [
    "memberjunction",
    "sql",
    "query",
    "generation",
    "ai",
    "templates"
  ],
  "author": "MemberJunction",
  "license": "MIT",
  "dependencies": {
    "@memberjunction/ai": "5.2.0",
    "@memberjunction/ai-core-plus": "5.2.0",
    "@memberjunction/aiengine": "5.2.0",
    "@memberjunction/ai-prompts": "5.2.0",
    "@memberjunction/ai-vectors-memory": "5.2.0",
    "@memberjunction/core": "5.2.0",
    "@memberjunction/core-entities": "5.2.0",
    "@memberjunction/global": "5.2.0",
    "@memberjunction/sqlserver-dataprovider": "5.2.0",
    "chalk": "^5.6.2",
    "commander": "^14.0.3",
    "nunjucks": "^3.2.4",
    "ora": "^9.3.0"
  },
  "devDependencies": {
    "@types/node": "24.10.11",
    "@types/nunjucks": "^3.2.6",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3",
    "vitest": "^3.1.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MemberJunction/MJ"
  }
}
