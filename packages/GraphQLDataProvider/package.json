{
  "name": "@memberjunction/graphql-dataprovider",
  "version": "3.0.0",
  "description": "MemberJunction: GraphQL Client Data Provider",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.cts",
  "exports": {
    "require": {
      "types": "./dist/index.d.cts",
      "default": "./dist/index.cjs"
    },
    "import": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    }
  },
  "type": "module",
  "files": [
    "/dist"
  ],
  "scripts": {
    "build": "tsc -noEmit && pkgroll --sourcemap --minify",
    "build-debug": "tsc -noEmit && pkgroll --sourcemap=inline --no-minify"
  },
  "author": "MemberJunction.com",
  "license": "ISC",
  "devDependencies": {
    "pkgroll": "2.1.1",
    "typescript": "^5.4.5"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "4.17.2",
    "@esbuild/win32-x64": "0.24.0"
  },
  "dependencies": {
    "@memberjunction/actions-base": "3.0.0",
    "@memberjunction/ai-core-plus": "3.0.0",
    "@memberjunction/core": "3.0.0",
    "@memberjunction/core-entities": "3.0.0",
    "@memberjunction/global": "3.0.0",
    "@memberjunction/interactive-component-types": "3.0.0",
    "@tempfix/idb": "^8.0.3",
    "graphql": "^16.8.0",
    "graphql-request": "^5.2.0",
    "graphql-ws": "^5.14.0",
    "rxjs": "^7.8.1",
    "uuid": "^10.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MemberJunction/MJ"
  }
}
