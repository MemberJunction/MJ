{
  "name": "mj_codegen_api",
  "version": "1.0.0",
  "description": "API Engine for MemberJunction CodeGen",
  "main": "src/index.ts",
  "private": true,
  "scripts": {
    "dev:debug": "ts-node-dev --transpile-only --respawn --inspect=4321 --project tsconfig.json src/index.ts",
    "dev": "ts-node-dev --transpile-only --respawn --project tsconfig.json src/index.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prestart": "mj codegen manifest --exclude-packages @memberjunction --output ./src/generated/class-registrations-manifest.ts",
    "prebuild": "mj codegen manifest --exclude-packages @memberjunction --output ./src/generated/class-registrations-manifest.ts",
    "build": "tsc && tsc-alias -f",
    "watch": "tsc -w",
    "clean": "rimraf output",
    "start": "node dist/index.js",
    "lint:eslint": "eslint \"**/*.{ts,js,}\" --max-warnings=0",
    "lint:tsc": "tsc --noemit",
    "lint": "concurrently \"npm:lint:eslint\" \"npm:lint:tsc\"",
    "zip": "zip -qr deploy.zip ./node_modules ./dist ./package.json"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@memberjunction/core": "3.4.0",
    "@memberjunction/global": "3.4.0",
    "@memberjunction/server-bootstrap-lite": "3.4.0",
    "@memberjunction/core-entities": "3.4.0",
    "@memberjunction/sqlserver-dataprovider": "3.4.0",
    "@memberjunction/codegen-lib": "3.4.0",
    "@types/axios": "^0.14.4",
    "@types/mssql": "^9.1.9",
    "axios": "^1.13.4",
    "compression": "^1.8.1",
    "dotenv": "^17.2.4",
    "env-var": "^7.4.1",
    "express": "^5.2.1",
    "fs-extra": "^11.3.3",
    "reflect-metadata": "0.2.2",
    "rxjs": "^7.8.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3"
  },
  "devDependencies": {
    "@types/compression": "^1.8.1",
    "@types/express": "^5.0.6",
    "@types/node": "24.10.11",
    "rimraf": "6.1.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MemberJunction/MJ"
  }
}
