{
  "name": "@memberjunction/ng-explorer-app",
  "version": "3.4.0",
  "description": "MemberJunction Explorer Application Shell - Complete branded entry point for Explorer-style applications",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "npm run clean && ngc && npm run copy-assets",
    "copy-assets": "cpy 'src/lib/styles/**' dist/styles --flat && cpy 'src/lib/assets/**' dist/assets && cpy src/lib/styles.scss dist --flat",
    "watch": "ngc --watch",
    "clean": "rimraf dist"
  },
  "keywords": [
    "memberjunction",
    "angular",
    "explorer",
    "application",
    "shell"
  ],
  "author": "MemberJunction",
  "license": "MIT",
  "dependencies": {
    "@memberjunction/core": "3.4.0",
    "@memberjunction/ng-auth-services": "3.4.0",
    "@memberjunction/ng-bootstrap": "3.4.0",
    "@memberjunction/ng-explorer-core": "3.4.0",
    "@memberjunction/ng-workspace-initializer": "3.4.0"
  },
  "peerDependencies": {
    "@angular/common": "^21.0.0",
    "@angular/core": "^21.0.0",
    "@angular/router": "^21.0.0",
    "rxjs": "^7.8.2"
  },
  "devDependencies": {
    "@angular/common": "^21.1.3",
    "@angular/core": "^21.1.3",
    "@angular/compiler": "^21.1.3",
    "@angular/compiler-cli": "^21.1.3",
    "typescript": "~5.9.3"
  },
  "files": [
    "dist/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/MemberJunction/MJ"
  }
}
