{
  "extends": "../../../tsconfig.angular.json",
  "compilerOptions": {
    "outDir": "./dist",
    "module": "ESNext",
    "composite": true,
    "emitDecoratorMetadata": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "removeComments": true,
    "noImplicitAny": false,
    "strictNullChecks": false,
    "strict": false
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules", "dist", "**/*.spec.ts"]
}
