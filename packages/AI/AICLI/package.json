{
  "name": "@memberjunction/ai-cli",
  "version": "3.2.0",
  "description": "MemberJunction AI agent, prompt and action execution CLI, integrated with the main MJ CLI.",
  "keywords": [
    "oclif",
    "ai",
    "agents",
    "actions"
  ],
  "homepage": "https://github.com/MemberJunction/MJ",
  "bugs": {
    "url": "https://github.com/MemberJunction/MJ/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MemberJunction/MJ"
  },
  "license": "ISC",
  "author": "MemberJunction",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "mj-ai": "bin/run.js"
  },
  "files": [
    "/bin",
    "/dist",
    "/oclif.manifest.json"
  ],
  "scripts": {
    "build": "tsc -b",
    "prepack": "npm run build && oclif manifest && oclif readme",
    "postpack": "rimraf oclif.manifest.json",
    "version": "oclif readme && git add README.md"
  },
  "oclif": {
    "bin": "mj-ai",
    "commands": "./dist/commands",
    "dirname": "mj-ai",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-warn-if-update-available",
      "@oclif/plugin-version"
    ],
    "warn-if-update-available": {
      "timeoutInDays": 1
    }
  },
  "dependencies": {
    "@inquirer/prompts": "^5.0.1",
    "@memberjunction/actions": "3.2.0",
    "@memberjunction/ai": "3.2.0",
    "@memberjunction/ai-agents": "3.2.0",
    "@memberjunction/ai-anthropic": "3.2.0",
    "@memberjunction/ai-betty-bot": "3.2.0",
    "@memberjunction/ai-cerebras": "3.2.0",
    "@memberjunction/ai-core-plus": "3.2.0",
    "@memberjunction/ai-groq": "3.2.0",
    "@memberjunction/ai-mistral": "3.2.0",
    "@memberjunction/ai-openai": "3.2.0",
    "@memberjunction/ai-prompts": "3.2.0",
    "@memberjunction/core": "3.2.0",
    "@memberjunction/core-actions": "3.2.0",
    "@memberjunction/core-entities": "3.2.0",
    "@memberjunction/core-entities-server": "3.2.0",
    "@memberjunction/sqlserver-dataprovider": "3.2.0",
    "@oclif/core": "^3",
    "@oclif/plugin-help": "^6",
    "@oclif/plugin-version": "^2.0.17",
    "@oclif/plugin-warn-if-update-available": "^3.0.16",
    "chalk": "^4.1.2",
    "cosmiconfig": "9.0.0",
    "dotenv": "16.4.5",
    "mssql": "^11.0.1",
    "ora-classic": "^5.4.2",
    "readline": "^1.3.0",
    "table": "^6.8.1"
  },
  "devDependencies": {
    "@oclif/prettier-config": "^0.2.1",
    "oclif": "^4",
    "rimraf": "5.0.7",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
