{
  "name": "@memberjunction/ai-cli",
  "type": "module",
  "version": "5.3.0",
  "description": "MemberJunction AI agent, prompt and action execution CLI, integrated with the main MJ CLI.",
  "keywords": [
    "oclif",
    "ai",
    "agents",
    "actions"
  ],
  "homepage": "https://github.com/MemberJunction/MJ",
  "bugs": {
    "url": "https://github.com/MemberJunction/MJ/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MemberJunction/MJ"
  },
  "license": "ISC",
  "author": "MemberJunction",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "mj-ai": "bin/run.js"
  },
  "files": [
    "/bin",
    "/dist",
    "/oclif.manifest.json"
  ],
  "scripts": {
    "build": "tsc -b && tsc-alias -f",
    "prepack": "npm run build && oclif manifest && oclif readme",
    "postpack": "rimraf oclif.manifest.json",
    "version": "oclif readme && git add README.md",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "oclif": {
    "bin": "mj-ai",
    "commands": "./dist/commands",
    "dirname": "mj-ai",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-warn-if-update-available",
      "@oclif/plugin-version"
    ],
    "warn-if-update-available": {
      "timeoutInDays": 1
    }
  },
  "dependencies": {
    "@inquirer/prompts": "^8.2.0",
    "@memberjunction/actions": "5.3.0",
    "@memberjunction/ai": "5.3.0",
    "@memberjunction/ai-agents": "5.3.0",
    "@memberjunction/ai-anthropic": "5.3.0",
    "@memberjunction/ai-betty-bot": "5.3.0",
    "@memberjunction/ai-cerebras": "5.3.0",
    "@memberjunction/ai-core-plus": "5.3.0",
    "@memberjunction/ai-groq": "5.3.0",
    "@memberjunction/ai-mistral": "5.3.0",
    "@memberjunction/ai-openai": "5.3.0",
    "@memberjunction/ai-prompts": "5.3.0",
    "@memberjunction/core": "5.3.0",
    "@memberjunction/core-actions": "5.3.0",
    "@memberjunction/core-entities": "5.3.0",
    "@memberjunction/core-entities-server": "5.3.0",
    "@memberjunction/sqlserver-dataprovider": "5.3.0",
    "@oclif/core": "^3.27.0",
    "@oclif/plugin-help": "^6.2.37",
    "@oclif/plugin-version": "^2.2.36",
    "@oclif/plugin-warn-if-update-available": "^3.1.55",
    "chalk": "^5.6.2",
    "cosmiconfig": "9.0.0",
    "dotenv": "17.2.4",
    "mssql": "^12.2.0",
    "ora-classic": "^5.4.2",
    "readline": "^1.3.0",
    "table": "^6.9.0"
  },
  "devDependencies": {
    "@oclif/prettier-config": "^0.2.1",
    "oclif": "^4.22.73",
    "rimraf": "6.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
