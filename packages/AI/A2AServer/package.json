{
  "name": "@memberjunction/a2aserver",
  "version": "5.3.0",
  "description": "MemberJunction Agent-To-Agent (A2A) Server Implementation",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "prestart": "mj codegen manifest --exclude-packages @memberjunction --output ./src/generated/class-registrations-manifest.ts",
    "prebuild": "mj codegen manifest --exclude-packages @memberjunction --output ./src/generated/class-registrations-manifest.ts || echo 'Warning: mj codegen manifest not available, using existing manifest'",
    "build": "tsc && tsc-alias -f",
    "clean": "rimraf dist",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [
    "MemberJunction",
    "A2A",
    "Agent-to-Agent",
    "AI"
  ],
  "author": "MemberJunction",
  "license": "MIT",
  "dependencies": {
    "@memberjunction/ai": "5.3.0",
    "@memberjunction/ai-agents": "5.3.0",
    "@memberjunction/api-keys": "5.3.0",
    "@memberjunction/ai-core-plus": "5.3.0",
    "@memberjunction/aiengine": "5.3.0",
    "@memberjunction/config": "5.3.0",
    "@memberjunction/core": "5.3.0",
    "@memberjunction/core-entities": "5.3.0",
    "@memberjunction/encryption": "5.3.0",
    "@memberjunction/global": "5.3.0",
    "@memberjunction/server": "5.3.0",
    "@memberjunction/server-bootstrap-lite": "5.3.0",
    "@memberjunction/sqlserver-dataprovider": "5.3.0",
    "cosmiconfig": "^9.0.0",
    "dotenv": "^17.2.4",
    "express": "^5.2.1",
    "mssql": "^12.2.0",
    "zod": "~3.24.4"
  },
  "devDependencies": {
    "@types/express": "^5.0.6",
    "@types/node": "^24.10.11",
    "rimraf": "^6.1.2",
    "typescript": "^5.9.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MemberJunction/MJ"
  }
}
