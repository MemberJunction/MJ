{
  "name": "@memberjunction/ai-mcp-server",
  "version": "4.4.0",
  "description": "MemberJunction: Model Context Protocol (MCP) - Server Implementation",
  "type": "module",
  "bin": {
    "MemberJunction": "./dist/index.js"
  },
  "scripts": {
    "prestart": "mj codegen manifest --exclude-packages @memberjunction --output ./src/generated/class-registrations-manifest.ts",
    "prebuild": "mj codegen manifest --exclude-packages @memberjunction --output ./src/generated/class-registrations-manifest.ts || echo 'Warning: mj codegen manifest not available, using existing manifest'",
    "build": "tsc && tsc-alias -f && node -e \"if (process.platform !== 'win32') require('fs').chmodSync('dist/index.js', 0o755)\"",
    "start": "node dist/index.js",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "files": [
    "dist"
  ],
  "author": "MemberJunction.com",
  "license": "ISC",
  "dependencies": {
    "@memberjunction/actions": "4.4.0",
    "@memberjunction/actions-base": "4.4.0",
    "@memberjunction/ai": "4.4.0",
    "@memberjunction/ai-agents": "4.4.0",
    "@memberjunction/ai-core-plus": "4.4.0",
    "@memberjunction/ai-prompts": "4.4.0",
    "@memberjunction/ai-provider-bundle": "4.4.0",
    "@memberjunction/aiengine": "4.4.0",
    "@memberjunction/api-keys": "4.4.0",
    "@memberjunction/config": "4.4.0",
    "@memberjunction/core": "4.4.0",
    "@memberjunction/core-entities": "4.4.0",
    "@memberjunction/credentials": "4.4.0",
    "@memberjunction/encryption": "4.4.0",
    "@memberjunction/global": "4.4.0",
    "@memberjunction/server": "4.4.0",
    "@memberjunction/server-bootstrap-lite": "4.4.0",
    "@memberjunction/sqlserver-dataprovider": "4.4.0",
    "@modelcontextprotocol/sdk": "^1.26.0",
    "cosmiconfig": "9.0.0",
    "dotenv": "^17.2.4",
    "express": "^5.2.1",
    "jsonwebtoken": "9.0.3",
    "jwks-rsa": "^3.2.2",
    "mssql": "^12.2.0",
    "yargs": "^18.0.0",
    "zod": "~3.24.4"
  },
  "devDependencies": {
    "@types/express": "^5.0.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/yargs": "^17.0.35",
    "@types/node": "24.10.11",
