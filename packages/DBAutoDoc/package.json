{
  "name": "@memberjunction/db-auto-doc",
  "version": "2.114.0",
  "description": "AI-powered SQL Server database documentation generator. Analyzes your database structure, uses AI to generate comprehensive descriptions, and saves them as extended properties. Works standalone - no MemberJunction runtime required.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "db-auto-doc": "bin/run.js"
  },
  "oclif": {
    "bin": "db-auto-doc",
    "commands": "./dist/commands",
    "dirname": "db-auto-doc",
    "topicSeparator": " ",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "files": [
    "/bin",
    "/dist"
  ],
  "scripts": {
    "start": "ts-node-dev src/cli/cli.ts",
    "build": "tsc",
    "watch": "tsc --watch",
    "clean": "rm -rf dist",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "sql-server",
    "database",
    "documentation",
    "ai",
    "metadata",
    "extended-properties",
    "database-documentation",
    "sql-server-documentation",
    "auto-documentation"
  ],
  "author": "MemberJunction.com",
  "license": "MIT",
  "devDependencies": {
    "@types/inquirer": "^9.0.7",
    "@types/mssql": "^9.1.1",
    "@types/node": "^20.10.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@inquirer/prompts": "^5.0.6",
    "@oclif/core": "^3.27.0",
    "@oclif/plugin-help": "^6.2.33",
    "chalk": "^4.1.2",
    "commander": "^11.1.0",
    "dotenv": "^16.3.1",
    "inquirer": "^8.2.5",
    "mssql": "^10.0.1",
    "ora": "^5.4.1",
    "zod": "^3.22.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MemberJunction/MJ"
  }
}
