{
  "name": "@memberjunction/scheduled-actions-server",
  "type": "module",
  "version": "4.0.0",
  "description": "Simple application server that can be called via URL to invoke Scheduled Actions",
  "main": "src/index.ts",
  "scripts": {
    "dev:debug": "ts-node-dev --transpile-only --respawn --inspect=4321 --project tsconfig.json src/index.ts",
    "dev": "ts-node-dev --transpile-only --respawn --project tsconfig.json src/index.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc && tsc-alias -f",
    "watch": "tsc -w",
    "clean": "rimraf output",
    "start": "ts-node-dev src/index.ts",
    "lint:eslint": "eslint \"**/*.{ts,js,}\" --max-warnings=0",
    "lint:tsc": "tsc --noemit",
    "lint": "concurrently \"npm:lint:eslint\" \"npm:lint:tsc\"",
    "zip": "zip -qr deploy.zip ./node_modules ./dist ./package.json"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@memberjunction/actions": "4.0.0",
    "@memberjunction/actions-apollo": "4.0.0",
    "@memberjunction/actions-content-autotag": "4.0.0",
    "@memberjunction/ai": "4.0.0",
    "@memberjunction/ai-mistral": "4.0.0",
    "@memberjunction/ai-openai": "4.0.0",
    "@memberjunction/ai-vector-sync": "4.0.0",
    "@memberjunction/ai-vectors-pinecone": "4.0.0",
    "@memberjunction/core": "4.0.0",
    "@memberjunction/core-entities": "4.0.0",
    "@memberjunction/scheduled-actions": "4.0.0",
    "@memberjunction/sqlserver-dataprovider": "4.0.0",
    "@types/axios": "^0.14.4",
    "dotenv": "^17.2.4",
    "env-var": "^7.3.0",
    "express": "^5.2.1",
    "mssql": "^12.2.0",
    "typescript": "^5.9.3"
  },
  "devDependencies": {
    "@types/express": "^5.0.6",
    "@types/node": "^24.10.11",
    "rimraf": "^6.1.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MemberJunction/MJ"
  }
}
