{
  "name": "@memberjunction/scheduled-actions-server",
  "version": "2.3.0",
  "description": "Simple application server that can be called via URL to invoke Scheduled Actions",
  "main": "src/index.ts",
  "scripts": {
    "dev:debug": "ts-node-dev --transpile-only --respawn --inspect=4321 --project tsconfig.json src/index.ts",
    "dev": "ts-node-dev --transpile-only --respawn --project tsconfig.json src/index.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc",
    "watch": "tsc -w",
    "clean": "rimraf output",
    "start": "ts-node-dev src/index.ts",
    "lint:eslint": "eslint \"**/*.{ts,js,}\" --max-warnings=0",
    "lint:tsc": "tsc --noemit",
    "lint": "concurrently \"npm:lint:eslint\" \"npm:lint:tsc\"",
    "zip": "zip -qr deploy.zip ./node_modules ./dist ./package.json"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@memberjunction/core": "2.3.0",
    "@memberjunction/core-entities": "2.3.0",
    "@memberjunction/actions": "2.3.0",
    "@memberjunction/scheduled-actions": "2.3.0",
    "@memberjunction/ai": "2.3.0",
    "@memberjunction/ai-vector-sync": "2.3.0",
    "@memberjunction/ai-vectors-pinecone": "2.3.0",
    "@memberjunction/ai-mistral": "2.3.0",
    "@memberjunction/ai-openai": "2.3.0",
    "@types/axios": "^0.14.0",
    "env-var": "^7.3.0",
    "express": "^4.18.2",
    "typescript": "^4.9.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^18.15.11",
    "rimraf": "^4.4.0"
  }
}
