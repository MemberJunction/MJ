{
  "name": "@memberjunction/react-runtime",
  "version": "3.4.0",
  "description": "Platform-agnostic React component runtime for MemberJunction. Provides core compilation, registry, and execution capabilities for React components in any JavaScript environment.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "npm run build:node && npm run build:umd",
    "build:node": "tsc",
    "build:umd": "webpack --config webpack.umd.config.js",
    "build:clean": "rimraf ./dist && npm run build",
    "watch": "tsc -w",
    "patchVersion": "npm version patch",
    "test": "jest"
  },
  "keywords": [
    "memberjunction",
    "react",
    "runtime",
    "component",
    "compiler"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MemberJunction/MJ/issues"
  },
  "homepage": "https://github.com/MemberJunction/MJ#readme",
  "dependencies": {
    "@memberjunction/core": "3.4.0",
    "@memberjunction/global": "3.4.0",
    "@memberjunction/interactive-component-types": "3.4.0",
    "@memberjunction/core-entities": "3.4.0",
    "@memberjunction/graphql-dataprovider": "3.4.0",
    "@babel/standalone": "^7.29.1",
    "rxjs": "^7.8.2"
  },
  "devDependencies": {
    "@types/node": "24.10.11",
    "jest": "^30.2.0",
    "rimraf": "^6.1.2",
    "typescript": "~5.9.3",
    "webpack": "^5.105.0",
    "webpack-cli": "^6.0.1",
    "babel-loader": "^10.0.0",
    "@babel/core": "^7.29.0",
    "@babel/preset-env": "^7.29.0",
    "path-browserify": "^1.0.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MemberJunction/MJ"
  }
}
