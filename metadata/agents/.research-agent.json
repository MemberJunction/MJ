      "Status": "Active",
      "DefaultArtifactTypeID": "@lookup:MJ: Artifact Types.Name=Research Content",
      "ExecutionOrder": 0,
      "ModelSelectionMode": "Agent",
      "ExposeAsAction": true,
      "IconClass": "fa-solid fa-magnifying-glass-chart",
      "PayloadSelfWritePaths": [
        "metadata.*",
        "plan.*",
        "findings.*",
        "comparisons.*",
        "contradictions.*",
        "iterations.*"
      ],
      "FinalPayloadValidation": "@file:validation/research-agent-validation.json",
      "FinalPayloadValidationMode": "Retry",
      "FinalPayloadValidationMaxRetries": 3
