{
  "entity": "MJ: User Notification Types",
  "filePattern": "**/.*.json",
  "ignoreDirectories": [
    "templates"
  ],
  "pull": {
    "createNewFileIfNotFound": true,
    "newFileName": ".notification-types.json",
    "appendRecordsToExistingFile": true,
    "updateExistingRecords": true,
    "preserveFields": [],
    "excludeFields": [],
    "mergeStrategy": "merge",
    "backupBeforeUpdate": true,
    "backupDirectory": ".backups",
    "ignoreNullFields": true,
    "ignoreVirtualFields": true,
    "lookupFields": {
      "EmailTemplateID": {
        "entity": "Templates",
        "field": "Name"
      },
      "SMSTemplateID": {
        "entity": "Templates",
        "field": "Name"
      }
    }
  }
}
