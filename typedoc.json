{
  // Comments are supported
  "name": "Member Junction",
  "entryPoints": [
    "packages/*",
    "packages/AI/*",
    "packages/AI/Providers/*",
    "packages/AI/Recommendations/*",
    "packages/AI/Vectors/*",
    "packages/Angular/*",
    "packages/Angular/Explorer/*",
    "packages/Angular/Generic/*",
    "packages/Actions/*",
    "packages/Communication/*",
    "packages/Templates/*"
  ],
  "entryPointStrategy": "packages",
  "out": "docs",
  "exclude": [
    "packages/MJCLI/",
    "packages/CodeGen/",
    "packages/GeneratedEntities/",
    "packages/GeneratedActions/",
    "packages/MJAPI/",
    "packages/MJExplorer/",
    "**/*.spec.ts",
    "**/*.d.ts",
    "node_modules"
  ]
}
